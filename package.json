{
  "name": "3dtile-viewer",
  "displayName": "3dtile-viewer",
  "description": "3DTile-Viewer is a tool for viewing 3dtile data in the vscode editor. The data formats you can view are.json,.pnts,.b3dm,.i3dm,.cmpt.",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:3dtile-viewer.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "3dtile-viewer.helloWorld",
        "title": "tile-viewer"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          
          "command": "3dtile-viewer.helloWorld",
          "group": "group1",
          "when": "resourceExtname== '.json' || resourceExtname== '.b3dm' || resourceExtname== '.i3dm' || resourceExtname== '.pnts' || resourceExtname== '.cmpt'"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "vue": "^3.3.4",
    "vue-loader": "^17.2.2",
    "vue-template-compiler": "^2.7.14"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "3d-tiles-renderer": "^0.3.20",
    "cesium": "^1.107.0",
    "lil-gui": "^0.18.2",
    "three": "^0.154.0",
    "three-mesh-bvh": "^0.6.1",
    "vite": "^4.4.2"
  },
  "publisher": "chenxuanxuan",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangchuan12/3dtile-viewer"
  }
}
